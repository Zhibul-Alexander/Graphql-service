{
    "compilerOptions": {
      "target": "ES6",
      "module": "Node16",
      "moduleResolution": "Node",
      "outDir": "./dist",
      "resolveJsonModule": true,
      "esModuleInterop": true,
      "noImplicitAny": true,
      "skipLibCheck": true,
      "strict": true,
    },
    "include": ["src/**/*"],
    "exclude": ["node_modules"]
}